{
  "name": "appsync-typescript-template",
  "version": "0.1.0",
  "bin": {
    "appsync-typescript-template": "bin/appsync-typescript-template.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:unit:coverage": "jest --coverage test/unit",
    "test:integration:coverage": "node ./test/integration/helpers/coverage",
    "cdk": "cdk",
    "clean": "tsc --build --clean",
    "lint": "eslint . --ext .ts",
    "test:perf": "npm run build && k6 run test/performance/index.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@graphql-tools/schema": "^9.0.16",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "aws-amplify": "^5.0.16",
    "aws-cdk": "2.69.0",
    "console-table-printer": "^2.11.1",
    "datadog-cdk-constructs-v2": "^1.4.0",
    "dd-trace": "^3.11.0",
    "esbuild": "^0.15.15",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4",
    "ws": "^8.12.1"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.5.0",
    "@middy/core": "^4.0.9",
    "aws-cdk-lib": "^2.69.0",
    "constructs": "^10.4.2",
    "launchdarkly-node-server-sdk": "^7.0.0",
    "source-map-support": "^0.5.21"
  }
}
